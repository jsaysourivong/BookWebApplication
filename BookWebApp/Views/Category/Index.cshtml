@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Category</h1>

<table class = "table table-bordered table-danger-striped" style="width:100%">
    <thread>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display Order
            </th>
        </tr>
    </thread>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width = "50">
                    @obj.Name
                </td>
                <td width = "30">
                    @obj.DisplayOrder
                </td>
            </tr>
        }
    </tbody>
</table>


